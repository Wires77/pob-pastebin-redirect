{
  "name": "PoB Pastebin Redirect",
  "description": "Open Pastebin links in PoB directly",
  "version": "0.1.2",
  "manifest_version": 2,
  "background": { "scripts": ["background.js"] },
  "icons": {
    "48": "img/pobfork-48.png",
    "128": "img/pobfork-128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://pastebin.com/*"
  ]
}
