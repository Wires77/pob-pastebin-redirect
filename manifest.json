{
  "name": "PoB Pastebin Redirect",
  "description": "Open Pastebin links in PoB directly",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": { "scripts": ["background.js"] },
  "icons": {
    "128": "img/pobfork.ico"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://pastebin.com/*"
  ]
}
